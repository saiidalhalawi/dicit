{
  "manifest_version": 2,
  "name": "dicit",
  "description": "put a random quote into every new tab",
  "version": "1.0",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "quote.html"
  }
}
